{
  "name": "@anvaka/streamlines",
  "version": "1.3.0",
  "description": "Streamlines calculator",
  "main": "index.js",
  "scripts": {
    "test": "tap --branches=60 --lines=60 --statements=60 --functions=60  test/*.js",
    "build": "browserify -s streamlines ./index.js > dist/streamlines.js && uglifyjs dist/streamlines.js > dist/streamlines.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anvaka/streamlines.git"
  },
  "keywords": [
    "streamlines",
    "vector",
    "field",
    "evenly",
    "spaced"
  ],
  "author": "Andrei Kashcha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anvaka/streamlines/issues"
  },
  "homepage": "https://github.com/anvaka/streamlines#readme",
  "devDependencies": {
    "tap": "^15.0.9",
    "uglify-js": "^3.13.9"
  }
}
